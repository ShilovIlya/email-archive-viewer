{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "eav",
            "kebab-case"
        ],
        "no-consecutive-blank-lines": [true, 2]
    }
}
